#include <stdio.h>

void main() {
    int n;
    printf("enter the size of an array: ");
    scanf("%d", &n);
    int arr[n];
    int i;
    printf("enter the element of an array: ");
    for(i = 0; i<n; i++){
        scanf("%d", &arr[i]);
    }
    for(i = 0; i<n; i++){
        printf("%d at %d position", arr[i], i);
    }
    
